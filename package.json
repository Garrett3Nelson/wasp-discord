		"@types/module-alias": "^2.0.4",
		"@types/node": "^20.14.13",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^8.10.0",
		"module-alias": "^2.2.3",
		"prettier": "^2.8.8",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
