	"license": "LGPL-3.0-or-later",
	"main": "index.js",
	"scripts": {
		"format": "prettier --plugin-search-dir . --write .",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"fix": "prettier --plugin-search-dir . --write . && eslint . --fix",
